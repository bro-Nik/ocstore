<!--noindex-->
<nav id="mobil_mmenu" class="dnone">
	<ul id="panel-menu">
		{% if (revtheme_dop_menus or (cats_status and categories) or revtheme_header_menu_links) %} 					
			{% if (revtheme_dop_menus) %} 
				{% for revtheme_dop_menu in revtheme_dop_menus %} 
					<li>
						{% if revtheme_dop_menu['href'~config_language_id] %}
						<a href="{{ revtheme_dop_menu['href'~config_language_id] }}" rel="nofollow">{% if (revtheme_dop_menu['icontype'] == 'iconka' and revtheme_dop_menu['dop_menu_iconka'] is defined) %} <span class="am_category_image"><i class="{{ revtheme_dop_menu['dop_menu_iconka'] }}"></i></span> {% else %} {% if (revtheme_dop_menu['dop_menu_image']) %} <span class="am_category_image"><img src="{{ revtheme_dop_menu['dop_menu_image'] }}" alt=""></span> {% endif %} {% endif %} {{ revtheme_dop_menu['name'~config_language_id] }}</a>
							{% else %}
							<span>{% if (revtheme_dop_menu['icontype'] == 'iconka' and revtheme_dop_menu['dop_menu_iconka'] is defined) %} <span class="am_category_image"><i class="{{ revtheme_dop_menu['dop_menu_iconka'] }}"></i></span> {% else %} {% if (revtheme_dop_menu['dop_menu_image']) %} <span class="am_category_image"><img src="{{ revtheme_dop_menu['dop_menu_image'] }}" alt=""></span> {% endif %} {% endif %} {{ revtheme_dop_menu['name'~config_language_id] }}</span>
						{% endif %}
						{% if (revtheme_dop_menu['children'] is defined) %} 
							<ul class="list-unstyled">
								{% for child in revtheme_dop_menu['children'] %} 
									<li>
										{% if revtheme_dop_menu['href'~config_language_id] != '' %}
											<a href="{{ child['href'~config_language_id] }}" rel="nofollow">{{ child['name'~config_language_id] }}</a>
											{% else %}
											<span>{{ child['name'~config_language_id] }}</span>
										{% endif %}
										{% if (tri_level and child['children'] is defined) %}
											<ul class="list-unstyled">
												{% for child in child['children'] %}
													<li><a href="{{ child['href'~config_language_id] }}" rel="nofollow">{{ child['name'~config_language_id] }}</a></li>
												{% endfor %}
											</ul>
										{% endif %}
									</li>
								{% endfor %} 
							</ul>
						{% endif %} 
					</li>
				{% endfor %} 
			{% endif %}
			{% if (cats_status) %} 
				{% for category in categories %} 
					<li>							
						{% if (category['children']) %} 
							<span><a href="{{ category['href'] }}" rel="nofollow">{{ category['name'] }}</a></span>
							<ul>
								{% for child in category['children'] %} 
									<li>
										<a href="{{ child['href'] }}" rel="nofollow">{{ child['name'] }}</a>
										{% if (tri_level and child['children']) %} 
											<ul class="list-unstyled">
												{% for child in child['children'] %} 
													<li><a href="{{ child['href'] }}" rel="nofollow">{{ child['name'] }}</a></li>
												{% endfor %} 
											</ul>
										{% endif %} 
									</li>
								{% endfor %} 
							</ul>
						{% else %}
							<a href="{{ category['href'] }}" rel="nofollow">{{ category['name'] }}</a>									
						{% endif %} 
					</li>
				{% endfor %} 
			{% endif %} 
			{% if (revtheme_header_menu_links) %} 
				{% for revtheme_header_menu_link in revtheme_header_menu_links %} 
					<li><a href="{{ revtheme_header_menu_link['link'] }}" rel="nofollow">{{ revtheme_header_menu_link['title'] }}</a></li>
				{% endfor %} 
			{% endif %} 				
		{% endif %} 
		{% if (rev_acc) %} 
			<li><span><i class="fa fa-user"></i> {{ text_revlogged }}</span>
				<ul>
					{% if (logged) %} 
						<li><a href="{{ account }}" rel="nofollow">{{ text_account }}</a></li>
						{% if (setting_all_settings['revaccount_orders'] is defined and setting_all_settings['revaccount_orders']) %} 
							<li><a href="{{ order }}" rel="nofollow">{{ text_rev_text_order }}</a></li>
						{% endif %} 
						{% if (setting_all_settings['revaccount_files'] is defined and setting_all_settings['revaccount_files']) %} 
							<li><a href="{{ download }}" rel="nofollow">{{ text_rev_text_download }}</a></li>
						{% endif %} 
						<li><a href="{{ logout }}" rel="nofollow">{{ text_logout }}</a></li>
					{% else %} 
						{% if (popup_login) %} 
							<li><a href="javascript:void(0);" onclick="get_revpopup_login();" rel="nofollow">{{ text_login }}</a></li>
						{% else %} 
							<li><a href="{{ login }}" rel="nofollow">{{ text_login }}</a></li>
						{% endif %} 
						<li><a href="{{ register }}" rel="nofollow">{{ text_register }}</a></li>
					{% endif %} 
				</ul>
			</li>
		{% endif %}
		{% if (manuf_status) %} 
			<li><span><i class="fa fa-folder"></i> {{ text_revmenu_manufs }}</span>
				{% if (categories_m) %} 
					<ul>
						{% for category in categories_m %} 
								{% if (category['manufacturer']) %} 
									{% for manufacturers in category['manufacturer'] %} 
										<li><a href="{{ manufacturers['href'] }}" rel="nofollow">{{ manufacturers['name'] }}</a></li>
									{% endfor %} 
								{% endif %} 
						{% endfor %} 
					</ul>
				{% endif %} 
			</li>
		{% endif %} 
		{% if (informations or revtheme_header_links or revtheme_dop_menus_2 or revtheme_dop_menus_3) %} 
			<li><span>{{ text_header_information }}</span>					
				{% if (revtheme_dop_menus_2) %} 
					{% for revtheme_dop_menu in revtheme_dop_menus_2 %} 
						<li>
							<a href="{{ revtheme_dop_menu['href'~config_language_id] }}" rel="nofollow">{{ revtheme_dop_menu['name'~config_language_id] }}</a>
							{% if (revtheme_dop_menu['children'] is defined) %}
								<ul class="list-unstyled">
									{% for child in revtheme_dop_menu['children'] %}
										<li><a href="{{ child['href'~config_language_id] }}" rel="nofollow">{{ child['name'~config_language_id] }}</a></li>
									{% endfor %}
								</ul>
							{% endif %}
						</li>
					{% endfor %} 
				{% endif %} 
				{% if (revtheme_dop_menus_3) %} 
					{% for revtheme_dop_menu in revtheme_dop_menus_3 %} 
						<li>								
							{% if (revtheme_dop_menu['children'] is defined) %}
								<span><a href="{{ revtheme_dop_menu['href'~config_language_id] }}" rel="nofollow">{{ revtheme_dop_menu['name'~config_language_id] }}</a></span>
								<ul class="list-unstyled">
									{% for child in revtheme_dop_menu['children'] %}
										<li><a href="{{ child['href'~config_language_id] }}" rel="nofollow">{{ child['name'~config_language_id] }}</a></li>
									{% endfor %}
								</ul>
							{% else %} 
								<a href="{{ revtheme_dop_menu['href'~config_language_id] }}" rel="nofollow">{{ revtheme_dop_menu['name'~config_language_id] }}</a>
							{% endif %}
						</li>
					{% endfor %} 
				{% endif %} 					
			</li>
		{% endif %} 				
		{% for information in informations2 %} 
			<li><a href="{{ information['href'] }}" rel="nofollow">{{ information['title'] }}</a></li>
		{% endfor %} 
		{% if (revtheme_header_links2) %} 
			{% for revtheme_header_link2 in revtheme_header_links2 %} 
				<li><a href="{{ revtheme_header_link2['link'] }}" rel="nofollow">{{ revtheme_header_link2['title'] }}</a></li>
			{% endfor %} 
		{% endif %} 
		{% if (revblog_status and blog_categories) %} 
			{% for category in blog_categories %} 
				<li class="revblog_mobil_in_menu">
					<a href="{{ category['href'] }}" rel="nofollow">{{ category['name'] }}</a>
					{% if (category['children']) %} 
						<ul>
							{% for child in category['children'] %} 
								<li>
									<a href="{{ child['href'] }}" rel="nofollow">{{ child['name'] }}</a>
									{% if (tri_level and child['children']) %} 
										<ul class="list-unstyled">
											{% for child in child['children'] %} 
												<li><a href="{{ child['href'] }}" rel="nofollow">{{ child['name'] }}</a></li>
											{% endfor %} 
										</ul>
									{% endif %} 
								</li>
							{% endfor %} 
						</ul>
					{% endif %} 
				</li>
			{% endfor %} 
		{% endif %} 			
		{% if (revtheme_header_popupphone['status']) %} 
			<li class="Divider">
				<a class="mob_telef" onclick="get_revpopup_phone();"><i class="fa fa-phone" aria-hidden="true"></i> {{ text_header_revpopup_phone }}</a>
			</li>
		{% endif %} 
		{% if (header_phone_text) %} 
			<li class="Divider">{{ header_phone_text }}</li>
		{% endif %} 
		{% if (header_phone_text2) %} 
				<li class="Divider">{{ header_phone_text2 }}</li>
			{% endif %} 
		{% if (header_phone_number) %} 
			<li class="Divider">
				<a href="tel:{{ header_phone_cod is defined ? header_phone_cod : '' }}{{ header_phone_number }}" rel="nofollow">{% if (header_phone_cod) %}{{ header_phone_cod }}{% endif %} {{ header_phone_number }}</a>
			</li>
		{% endif %} 
		{% if (header_phone_number2) %} 
			<li class="Divider">
				<a href="tel:{{ header_phone_cod2 is defined ? header_phone_cod2 : '' }}{{ header_phone_number2 }}" rel="nofollow">{% if (header_phone_cod2) %}{{ header_phone_cod2 }}{% endif %} {{ header_phone_number2 }}</a>
			</li>
		{% endif %} 			
		{% if (phone_dop_text2|length > 30) %} 
			<li class="Divider wrspnormal">
				{{ phone_dop_text2 }} 
			</li>
		{% endif %} 
	</ul>
</nav>
<!--/noindex-->
